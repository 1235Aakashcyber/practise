#include<stdio.h>
void div(int a, int b);
void add(int a, int b);
void sub(int a, int b);
void mul(int a, int b);

int main(){
int a,b;
printf("Enter a :");
scanf("%d",&a);
printf("Enter b:");
scanf("%d",&b);

div(a,b);
add(a,b);
sub(a,b);
}
void add(a,b){
int sum = a+b;
printf("%d",sum);
}
void sub(int a, int b);
{
    int sub=a-b;
    printf("%d", sub);
}
void div(int a, int b){
    div=a/b;
    printf("%d",div);
}


